  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx server/index.ts",
    "build:server": "npx tsc -p tsconfig.server.json",
    "build:client": "vite build",
    "build": "npm run build:server && npm run build:client && npm run postbuild",
    "postbuild": "node ./scripts/fix-imports.cjs",
